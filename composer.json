{
    "name": "capeandbay/zingfit",
    "description": "A Laravel Package for ZingFit-enabled Projects.",
    "license": "MIT",
    "authors": [
        {
            "name": "Angel Gonzalez",
            "email": "angel@capeandbay.com",
            "homepage": "https://capeandbay.com"
        }
    ],
    "homepage": "https://github.com/capeandbay-devs/ZingFit",
    "keywords": ["Laravel", "ZingFit"],
    "require": {
        "illuminate/support": "~6|~7",
        "ixudra/curl": "^6.19",
        "goldspecdigital/laravel-eloquent-uuid": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CapeAndBay\\ZingFit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CapeAndBay\\ZingFit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CapeAndBay\\ZingFit\\ZingFitServiceProvider"
            ],
            "aliases": {
                "ZingFit": "CapeAndBay\\ZingFit\\Facades\\ZingFit"
            }
        }
    }
}
